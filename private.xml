<?xml version="1.0"?>
<root>

  <item>
    <name>EISUU to Option_L</name>
    <appendix>(+ When you type EISUU only, send EISUU)</appendix>
    <identifier>remap.jis_eisuu2optionL_eisuu</identifier>
    <autogen>__KeyOverlaidModifier__ KeyCode::JIS_EISUU, KeyCode::OPTION_L, KeyCode::JIS_EISUU</autogen>
  </item>

  <item>
    <name>Ctrl+j KANA</name>
    <identifier>private.ctrl_c</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::C, ModifierFlag::CONTROL_L,
      KeyCode::JIS_KANA,
    </autogen>
  </item>
  
  <item>
    <name>Right_CMD to EISUU + ESC</name>
    <appendix>(+ When you type CMD only, send CMD)</appendix>
    <identifier>remap.l_cmd2ESC_with_eisuu</identifier>
    <autogen>__KeyOverlaidModifier__ KeyCode::COMMAND_L, KeyCode::COMMAND_L, KeyCode::JIS_EISUU, KeyCode::ESCAPE</autogen>
  </item>


